INSERT INTO BulkUpload20170302 ( PatientID, Chromosome, Start19, Stop19, GenomeAssembly, Change, Chromosomal_sex, openaccess, DoB, [note], motheris, fatheris, inheritance, pathogenic, PrimaryPheno, [responsible contact], RequestedDate, IsMale, IsFemale, IsUnknown, PhenoFormID, SpecimenType )
SELECT Patients.PatientID, Chromosome.Chromosome, ArrayOligoPreliminaryResults.Start19, ArrayOligoPreliminaryResults.Stop19, "GRCh37/hg19" AS GenomeAssembly, Change.Change, '' AS Chromosomal_sex, '' AS openaccess, dbo_Patient.DoB, "" AS [note], "Of unknown phenotype" AS motheris, "Of unknown phenotype" AS fatheris, "Unknown" AS inheritance, Status.Status AS pathogenic, PhenoForm.PrimaryPheno, "" AS [responsible contact], dbo_ArrayTest.RequestedDate, Patients.Sexed, Patients.Sexed, Patients.Sexed, PhenoForm.PhenoFormID, dbo_SpecimenLinked.SpecimenType
FROM ((((dbo_Patient INNER JOIN ((Phenotype LEFT JOIN PhenoForm ON Phenotype.PhenoFormID = PhenoForm.PhenoFormID) INNER JOIN (Chromosome INNER JOIN (ArrayOligoPreliminaryResults INNER JOIN Patients ON ArrayOligoPreliminaryResults.InternalPatientID = Patients.InternalPatientID) ON Chromosome.ChrID = ArrayOligoPreliminaryResults.ChrID19) ON Phenotype.InternalPatientID = Patients.InternalPatientID) ON dbo_Patient.PatientTrustID = Patients.PatientID) INNER JOIN Status ON ArrayOligoPreliminaryResults.Pathogenic = Status.StatusID) INNER JOIN Change ON ArrayOligoPreliminaryResults.Copies = Change.ChangeID) INNER JOIN dbo_ArrayTest ON Patients.InternalPatientID = dbo_ArrayTest.InternalPatientID) INNER JOIN dbo_SpecimenLinked ON dbo_ArrayTest.SpecimenNo = dbo_SpecimenLinked.SpecimenTrustID
WHERE (((Patients.Sexed) Is Not Null) AND ((dbo_ArrayTest.Service)=True) AND ((dbo_ArrayTest.ReferralID)=1199901185) AND ((dbo_Patient.NHSNo) Is Not Null));
